LOCAL_DIR := $(GET_LOCAL_DIR)
TARGET := AGN_7130D_S55301A_E_DS1616
MODULES += app/mt_boot \
           dev/lcm
MTK_EMMC_SUPPORT = yes
DEFINES += MTK_NEW_COMBO_EMMC_SUPPORT
MTK_KERNEL_POWER_OFF_CHARGING = yes
MTK_LCM_PHYSICAL_ROTATION = 0
CUSTOM_LK_LCM = "ST7703_YIXINGGUANGDIAN_VO_MIPI4_HD_MINUS"
MTK_SECURITY_SW_SUPPORT = yes
MTK_SEC_FASTBOOT_UNLOCK_SUPPORT = yes
MTK_VERIFIED_BOOT_SUPPORT = yes
BOOT_LOGO:=hd640
#DEFINES += WITH_DEBUG_DCC=1
DEFINES += WITH_DEBUG_UART=1
#DEFINES += WITH_DEBUG_FBCON=1
#DEFINES += MACH_FPGA=y
#DEFINES += MACH_FPGA_NO_DISPLAY
DEFINES += AGENEW_LK_BOOT_VIBRATE_MDELAY=100
TRUSTONIC_TEE_SUPPORT=no
MTK_GOOGLE_TRUSTY_SUPPORT=no
MTK_USERIMAGES_USE_F2FS = yes
MTK_DM_VERITY_OFF = no
MTK_MRDUMP_ENABLE := no
MTK_AB_OTA_UPDATER = no
MTK_FAST_CHARGER_TECH = no
MICROTRUST_TEE_LITE_SUPPORT = yes